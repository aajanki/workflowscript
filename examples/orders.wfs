workflow main() {
  oid = "ord123"

  orders = getOrders(orderId=${oid})

  sys.log(text=${"Orders: " + orders})
}

workflow getOrders(orderId) {
  try {
    res = http.get(url=${"https://example.com/orders/" + orderId})
    return ${res}
  } catch (err) {
    if (${err.code == 404}) {
      return "Not found"
    } else {
      raise ${err}
    }
  }
}
